    21‚ÜíCHROMOSOME_PATTERNS: list[tuple[str, str]] = [
    22‚Üí    # Explicit chromosome names (most reliable)
    23‚Üí    (r&#x27;^chr(?:omosome)?[_\-\s]?(\d+|[XYZWAB]|MT?|Un)$&#x27;, &#x27;chr_explicit&#x27;),
