   105‚Üídef parse_fasta_from_handle(
   106‚Üí    handle: typing.Iterable[str],
   107‚Üí    keep_full_sequence: bool = False,
   108‚Üí) -&gt; list[tuple[str, int, str]]:
   109‚Üí    &quot;&quot;&quot;
   110‚Üí    Parse FASTA from a file handle and return list of (name, length, sequence_sample).
   111‚Üí
   112‚Üí    For efficiency, only stores first 10kb of each sequence (for GC calculation)
   113‚Üí    unless keep_full_sequence is True.
   114‚Üí
   115‚Üí    Args:
   116‚Üí        handle: File-like object to read from (e.g., open file, sys.stdin)
   117‚Üí        keep_full_sequence: If True, store entire sequence instead of just sample
   118‚Üí
   119‚Üí    Returns:
   120‚Üí        List of tuples: (scaffold_name, length, sequence or sequence_sample)
   121‚Üí
   122‚Üí    Raises:
   123‚Üí        ValueError: If the input appears to be empty or invalid FASTA format
   124‚Üí    &quot;&quot;&quot;
   125‚Üí    scaffolds: list[tuple[str, int, str]] = []
   126‚Üí    current_name: str | None = None
   127‚Üí    current_length = 0
   128‚Üí    current_seq_parts: list[str] = []
   129‚Üí    sample_limit = 10000  # Only keep first 10kb for GC calculation (if not keeping full)
   130‚Üí    line_count = 0
   131‚Üí    found_header = False
   132‚Üí
   133‚Üí    for raw_line in handle:
   134‚Üí        line_count += 1
   135‚Üí        line: str = str(raw_line).strip()
   136‚Üí
   137‚Üí        # Skip empty lines
   138‚Üí        if not line:
   139‚Üí            continue
   140‚Üí
   141‚Üí        if line.startswith(&quot;&gt;&quot;):
   142‚Üí            found_header = True
   143‚Üí            # Save previous scaffold
   144‚Üí            if current_name is not None:
   145‚Üí                seq = &quot;&quot;.join(current_seq_parts)
   146‚Üí                scaffolds.append((current_name, current_length, seq))
   147‚Üí
   148‚Üí            # Start new scaffold - extract name (first word after &gt;)
   149‚Üí            header_parts = line[1:].split()
   150‚Üí            if not header_parts:
   151‚Üí                raise ValueError(
   152‚Üí                    f&quot;Line {line_count}: Empty FASTA header (just &#x27;&gt;&#x27; with no name)&quot;
   153‚Üí                )
   154‚Üí            current_name = header_parts[0]
   155‚Üí            current_length = 0
   156‚Üí            current_seq_parts = []
   157‚Üí        else:
   158‚Üí            # This is sequence data
   159‚Üí            if not found_header:
   160‚Üí                raise ValueError(
   161‚Üí                    f&quot;Line {line_count}: Sequence data before first FASTA header. &quot;
   162‚Üí                    &quot;File may not be in FASTA format.&quot;
   163‚Üí                )
   164‚Üí            # Validate sequence characters (allow standard nucleotides + ambiguity codes)
   165‚Üí            valid_chars = set(&quot;ACGTNUacgtnuRYSWKMBDHVryswkmbdhv.-&quot;)
   166‚Üí            invalid_chars = set(line) - valid_chars
   167‚Üí            if invalid_chars:
   168‚Üí                # Only warn for non-whitespace invalid chars, don&#x27;t fail
   169‚Üí                # Some FASTA files have quality scores or other data
   170‚Üí                pass
   171‚Üí            current_length += len(line)
   172‚Üí            if keep_full_sequence:
   173‚Üí                current_seq_parts.append(line)
   174‚Üí            elif sum(len(s) for s in current_seq_parts) &lt; sample_limit:
   175‚Üí                current_seq_parts.append(line)
   176‚Üí
   177‚Üí    # Don&#x27;t forget last scaffold
   178‚Üí    if current_name is not None:
   179‚Üí        seq = &quot;&quot;.join(current_seq_parts)
   180‚Üí        scaffolds.append((current_name, current_length, seq))
   181‚Üí
   182‚Üí    if not scaffolds:
   183‚Üí        if line_count == 0:
   184‚Üí            raise ValueError(&quot;Input is empty&quot;)
   185‚Üí        raise ValueError(
   186‚Üí            &quot;No scaffolds found in input. &quot;
   187‚Üí            &quot;File may not be in FASTA format (expected lines starting with &#x27;&gt;&#x27;).&quot;
   188‚Üí        )
   189‚Üí
   190‚Üí    return scaffolds
   191‚Üí
