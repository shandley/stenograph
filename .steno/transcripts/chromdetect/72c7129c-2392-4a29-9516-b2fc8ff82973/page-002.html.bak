   222‚Üí          - pattern: &quot;...&quot;
   223‚Üí            name: &quot;...&quot;
   224‚Üí    &quot;&quot;&quot;
   225‚Üí    lines = content.strip().split(&quot;\n&quot;)
   226‚Üí    result: dict = {}
   227‚Üí    current_key: str | None = None
   228‚Üí    current_list: list = []
   229‚Üí    current_dict: dict = {}
   230‚Üí    indent_level = 0
   231‚Üí
   232‚Üí    for line in lines:
   233‚Üí        stripped = line.strip()
   234‚Üí        if not stripped or stripped.startswith(&quot;#&quot;):
   235‚Üí            continue
   236‚Üí
   237‚Üí        # Calculate indent
   238‚Üí        line_indent = len(line) - len(line.lstrip())
   239‚Üí
   240‚Üí        # Top-level key
   241‚Üí        if line_indent == 0 and &quot;:&quot; in stripped:
   242‚Üí            # Save previous list if any
   243‚Üí            if current_key and current_list:
   244‚Üí                result[current_key] = current_list
   245‚Üí                current_list = []
   246‚Üí            if current_key and current_dict:
   247‚Üí                current_list.append(current_dict)
   248‚Üí                result[current_key] = current_list
   249‚Üí                current_list = []
   250‚Üí                current_dict = {}
   251‚Üí
   252‚Üí            key = stripped.split(&quot;:&quot;)[0].strip()
   253‚Üí            value = &quot;:&quot;.join(stripped.split(&quot;:&quot;)[1:]).strip()
   254‚Üí            current_key = key
   255‚Üí            if value:
   256‚Üí                result[key] = value
   257‚Üí            indent_level = 0
   258‚Üí
   259‚Üí        # List item
   260‚Üí        elif stripped.startswith(&quot;-&quot;):
   261‚Üí            item_content = stripped[1:].strip()
   262‚Üí            # Check if it&#x27;s a key-value in list item
   263‚Üí            if &quot;:&quot; in item_content and not item_content.startswith(&#x27;&quot;&#x27;):
   264‚Üí                if current_dict:
   265‚Üí                    current_list.append(current_dict)
   266‚Üí                current_dict = {}
   267‚Üí                key, value = item_content.split(&quot;:&quot;, 1)
   268‚Üí                current_dict[key.strip()] = value.strip().strip(&#x27;&quot;&#x27;).strip(&quot;&#x27;&quot;)
   269‚Üí            else:
   270‚Üí                if current_dict:
   271‚Üí                    current_list.append(current_dict)
   272‚Üí                    current_dict = {}
   273‚Üí                current_list.append(item_content.strip(&#x27;&quot;&#x27;).strip(&quot;&#x27;&quot;))
   274‚Üí
   275‚Üí        # Continuation of dict item
   276‚Üí        elif &quot;:&quot; in stripped and current_dict is not None:
   277‚Üí            key, value = stripped.split(&quot;:&quot;, 1)
   278‚Üí            current_dict[key.strip()] = value.strip().strip(&#x27;&quot;&#x27;).strip(&quot;&#x27;&quot;)
   279‚Üí
   280‚Üí    # Save final items
   281‚Üí    if current_dict:
   282‚Üí        current_list.append(current_dict)
   283‚Üí    if current_key and current_list:
   284‚Üí        result[current_key] = current_list
   285‚Üí
   286‚Üí    return result
