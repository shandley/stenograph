   313‚Üí    compiled_custom_unloc = [re.compile(p, re.IGNORECASE) for p in custom_unloc]
   314‚Üí    compiled_custom_frag = [re.compile(p, re.IGNORECASE) for p in custom_frag]
   315‚Üí
