   229‚Üídef calculate_gc(sequence: str) -&gt; float | None:
   230‚Üí    &quot;&quot;&quot;Calculate GC content of a sequence.
   231‚Üí
   232‚Üí    Args:
   233‚Üí        sequence: DNA sequence string
   234‚Üí
   235‚Üí    Returns:
   236‚Üí        GC content as fraction (0.0-1.0), or None if sequence is empty
   237‚Üí    &quot;&quot;&quot;
   238‚Üí    if not sequence:
   239‚Üí        return None
   240‚Üí
   241‚Üí    sequence = sequence.upper()
   242‚Üí    gc = sequence.count(&quot;G&quot;) + sequence.count(&quot;C&quot;)
   243‚Üí    total = gc + sequence.count(&quot;A&quot;) + sequence.count(&quot;T&quot;)
   244‚Üí
   245‚Üí    return gc / total if total &gt; 0 else None
   246‚Üí
   247‚Üí
   248‚Üídef calculate_n50(lengths: list[int]) -&gt; int:
   249‚Üí    &quot;&quot;&quot;Calculate N50 from list of scaffold lengths.
   250‚Üí
   251‚Üí    N50 is the length such that 50% of the total assembly length
   252‚Üí    is contained in scaffolds of this length or longer.
   253‚Üí
   254‚Üí    Args:
   255‚Üí        lengths: List of scaffold lengths
   256‚Üí
   257‚Üí    Returns:
   258‚Üí        N50 value, or 0 if list is empty
   259‚Üí    &quot;&quot;&quot;
   260‚Üí    if not lengths:
   261‚Üí        return 0
   262‚Üí
   263‚Üí    sorted_lengths = sorted(lengths, reverse=True)
   264‚Üí    total = sum(sorted_lengths)
   265‚Üí    running_sum = 0
   266‚Üí
   267‚Üí    for length in sorted_lengths:
   268‚Üí        running_sum += length
   269‚Üí        if running_sum &gt;= total / 2:
   270‚Üí            return length
   271‚Üí
   272‚Üí    return sorted_lengths[-1]
   273‚Üí
   274‚Üí
   275‚Üídef calculate_n90(lengths: list[int]) -&gt; int:
   276‚Üí    &quot;&quot;&quot;Calculate N90 from list of scaffold lengths.
   277‚Üí
   278‚Üí    Args:
   279‚Üí        lengths: List of scaffold lengths
   280‚Üí
   281‚Üí    Returns:
   282‚Üí        N90 value, or 0 if list is empty
   283‚Üí    &quot;&quot;&quot;
   284‚Üí    if not lengths:
   285‚Üí        return 0
   286‚Üí
   287‚Üí    sorted_lengths = sorted(lengths, reverse=True)
   288‚Üí    total = sum(sorted_lengths)
   289‚Üí    running_sum = 0
   290‚Üí
   291‚Üí    for length in sorted_lengths:
   292‚Üí        running_sum += length
   293‚Üí        if running_sum &gt;= total * 0.9:
   294‚Üí            return length
   295‚Üí
   296‚Üí    return sorted_lengths[-1]
