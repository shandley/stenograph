import struct
import zlib

def create_rgba_png(width, height, color):
    def chunk(chunk_type, data):
        chunk_len = struct.pack(&#39;&gt;I&#39;, len(data))
        chunk_crc = struct.pack(&#39;&gt;I&#39;, zlib.crc32(chunk_type + data) &amp; 0xffffffff)
        return chunk_len + chunk_type + data + chunk_crc
    
    header = b&#39;\\x89PNG\\r\\n\\x1a\\n&#39;
    # 8 bits, color type 6 = RGBA
    ihdr = struct.pack(&#39;&gt;IIBBBBB&#39;, width, height, 8, 6, 0, 0, 0)
    
    raw_data = b&#39;&#39;
    for y in range(height):
        raw_data += b&#39;\\x00&#39;  # filter byte
        for x in range(width):
            raw_data += bytes(color)  # RGBA
    
    return header + chunk(b&#39;IHDR&#39;, ihdr) + chunk(b&#39;IDAT&#39;, zlib.compress(raw_data)) + chunk(b&#39;IEND&#39;, b&#39;&#39;)

# Blue with full alpha
rgba = [0, 120, 212, 255]

for name, size in [(&#39;icon.png&#39;, 32), (&#39;32x32.png&#39;, 32), (&#39;128x128.png&#39;, 128), (&#39;128x128@2x.png&#39;, 256)]:
    with open(name, &#39;wb&#39;) as f:
        f.write(create_rgba_png(size, size, rgba))

print(&#39;RGBA icons created&#39;)
