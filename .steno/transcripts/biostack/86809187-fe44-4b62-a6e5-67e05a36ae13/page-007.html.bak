<div class="truncatable"><div class="truncatable-content"><pre class="file-content">//! Content hashing utilities for provenance tracking.
//!
//! Content-addressable hashing enables:
//! - Cache lookup (skip re-computation if inputs match)
//! - Data integrity verification
//! - Deduplication of artifacts

use sha2::{Digest, Sha256};
use std::fmt;
use std::io::{self, Read};
use std::path::Path;

/// A content hash (SHA-256).
#[derive(Debug, Clone, PartialEq, Eq, Hash)]
pub struct Hash {
    bytes: [u8; 32],
}

impl Hash {
    /// Create a hash from raw bytes.
    pub fn from_bytes(bytes: [u8; 32]) -&gt; Self {
        Self { bytes }
    }

    /// Get the hash bytes.
    pub fn as_bytes(&amp;self) -&gt; &amp;[u8; 32] {
        &amp;self.bytes
    }

    /// Convert to a hex string.
    pub fn to_hex(&amp;self) -&gt; String {
        hex::encode(self.bytes)
    }

    /// Parse from a hex string.
    pub fn from_hex(s: &amp;str) -&gt; Result&lt;Self, HashError&gt; {
        // Strip optional &#34;sha256:&#34; prefix
        let hex_str = s.strip_prefix(&#34;sha256:&#34;).unwrap_or(s);

        let bytes = hex::decode(hex_str).map_err(|_| HashError::InvalidHex)?;
        if bytes.len() != 32 {
            return Err(HashError::InvalidLength);
        }
        let mut arr = [0u8; 32];
        arr.copy_from_slice(&amp;bytes);
        Ok(Self { bytes: arr })
    }
}

impl fmt::Display for Hash {
    fn fmt(&amp;self, f: &amp;mut fmt::Formatter&lt;&#39;_&gt;) -&gt; fmt::Result {
        write!(f, &#34;sha256:{}&#34;, self.to_hex())
    }
}

impl serde::Serialize for Hash {
    fn serialize&lt;S&gt;(&amp;self, serializer: S) -&gt; Result&lt;S::Ok, S::Error&gt;
    where
        S: serde::Serializer,
    {
        serializer.serialize_str(&amp;self.to_string())
    }
}

impl&lt;&#39;de&gt; serde::Deserialize&lt;&#39;de&gt; for Hash {
    fn deserialize&lt;D&gt;(deserializer: D) -&gt; Result&lt;Self, D::Error&gt;
    where
        D: serde::Deserializer&lt;&#39;de&gt;,
    {
        let s = String::deserialize(deserializer)?;
        Self::from_hex(&amp;s).map_err(serde::de::Error::custom)
    }
}

/// Errors that can occur during hashing.
#[derive(Debug, thiserror::Error)]
pub enum HashError {
    #[error(&#34;Invalid hex string&#34;)]
    InvalidHex,

    #[error(&#34;Invalid hash length (expected 32 bytes)&#34;)]
    InvalidLength,

    #[error(&#34;I/O error: {0}&#34;)]
    Io(#[from] io::Error),
}

/// Content hasher for computing SHA-256 hashes.
#[derive(Debug, Default)]
pub struct ContentHasher {
    hasher: Sha256,
}

impl ContentHasher {
    /// Create a new content hasher.
    pub fn new() -&gt; Self {
        Self {
            hasher: Sha256::new(),
        }
    }

    /// Update the hash with data.
    pub fn update(&amp;mut self, data: &amp;[u8]) {
        self.hasher.update(data);
    }

    /// Finalize and return the hash.
    pub fn finalize(self) -&gt; Hash {
        let result = self.hasher.finalize();
        Hash::from_bytes(result.into())
    }

    /// Hash a string directly.
    pub fn hash_string(s: &amp;str) -&gt; Hash {
        let mut hasher = Self::new();
        hasher.update(s.as_bytes());
        hasher.finalize()
    }

    /// Hash bytes directly.
    pub fn hash_bytes(data: &amp;[u8]) -&gt; Hash {
        let mut hasher = Self::new();
        hasher.update(data);
        hasher.finalize()
    }

    /// Hash a file.
    pub fn hash_file(path: &amp;Path) -&gt; Result&lt;Hash, HashError&gt; {
        let mut file = std::fs::File::open(path)?;
        let mut hasher = Self::new();
        let mut buffer = [0u8; 8192];

        loop {
            let bytes_read = file.read(&amp;mut buffer)?;
            if bytes_read == 0 {
                break;
            }
            hasher.update(&amp;buffer[..bytes_read]);
        }

        Ok(hasher.finalize())
    }

    /// Hash a JSON value (canonicalized).
    ///
    /// Uses a deterministic serialization to ensure the same
    /// logical value always produces the same hash.
    pub fn hash_json(value: &amp;serde_json::Value) -&gt; Hash {
        // Use compact JSON with sorted keys for determinism
        let canonical = canonicalize_json(value);
        Self::hash_string(&amp;canonical)
    }

    /// Combine multiple hashes into one.
    ///
    /// Useful for computing the hash of multiple inputs.
    pub fn combine(hashes: &amp;[Hash]) -&gt; Hash {
        let mut hasher = Self::new();
        for hash in hashes {
            hasher.update(hash.as_bytes());
        }
        hasher.finalize()
    }

    /// Hash primitive parameters for cache lookup.
    ///
    /// Combines the operation name with the parameters to create
    /// a unique key for caching.
    pub fn hash_operation(operation: &amp;str, params: &amp;serde_json::Value) -&gt; Hash {
        let mut hasher = Self::new();
        hasher.update(operation.as_bytes());
        hasher.update(b&#34;:&#34;);
        hasher.update(canonicalize_json(params).as_bytes());
        hasher.finalize()
    }
}

/// Canonicalize a JSON value for deterministic hashing.
///
/// - Objects have keys sorted alphabetically
/// - No extra whitespace
/// - Numbers are normalized
fn canonicalize_json(value: &amp;serde_json::Value) -&gt; String {
    match value {
        serde_json::Value::Null =&gt; &#34;null&#34;.to_string(),
        serde_json::Value::Bool(b) =&gt; b.to_string(),
        serde_json::Value::Number(n) =&gt; n.to_string(),
        serde_json::Value::String(s) =&gt; format!(&#34;\&#34;{}\&#34;&#34;, escape_json_string(s)),
        serde_json::Value::Array(arr) =&gt; {
            let items: Vec&lt;String&gt; = arr.iter().map(canonicalize_json).collect();
            format!(&#34;[{}]&#34;, items.join(&#34;,&#34;))
        }
        serde_json::Value::Object(obj) =&gt; {
            // Sort keys for determinism
            let mut keys: Vec&lt;_&gt; = obj.keys().collect();
            keys.sort();
            let items: Vec&lt;String&gt; = keys
                .iter()
                .map(|k| format!(&#34;\&#34;{}\&#34;:{}&#34;, escape_json_string(k), canonicalize_json(&amp;obj[*k])))
                .collect();
            format!(&#34;{{{}}}&#34;, items.join(&#34;,&#34;))
        }
    }
}

/// Escape special characters in a JSON string.
fn escape_json_string(s: &amp;str) -&gt; String {
    let mut result = String::with_capacity(s.len());
    for c in s.chars() {
        match c {
            &#39;&#34;&#39; =&gt; result.push_str(&#34;\\\&#34;&#34;),
            &#39;\\&#39; =&gt; result.push_str(&#34;\\\\&#34;),
            &#39;\n&#39; =&gt; result.push_str(&#34;\\n&#34;),
            &#39;\r&#39; =&gt; result.push_str(&#34;\\r&#34;),
            &#39;\t&#39; =&gt; result.push_str(&#34;\\t&#34;),
            c if c.is_control() =&gt; {
                result.push_str(&amp;format!(&#34;\\u{:04x}&#34;, c as u32));
            }
            c =&gt; result.push(c),
        }
    }
    result
}

#[cfg(test)]
mod tests {
    use super::*;
    use serde_json::json;

    #[test]
    fn test_hash_string() {
        let hash = ContentHasher::hash_string(&#34;hello world&#34;);
        assert_eq!(hash.to_hex().len(), 64); // 32 bytes = 64 hex chars
    }

    #[test]
    fn test_hash_determinism() {
        let h1 = ContentHasher::hash_string(&#34;test&#34;);
        let h2 = ContentHasher::hash_string(&#34;test&#34;);
        assert_eq!(h1, h2);
    }

    #[test]
    fn test_hash_display() {
        let hash = ContentHasher::hash_string(&#34;test&#34;);
        let s = hash.to_string();
        assert!(s.starts_with(&#34;sha256:&#34;));
    }

    #[test]
    fn test_hash_roundtrip() {
        let original = ContentHasher::hash_string(&#34;test&#34;);
        let hex = original.to_string();
        let parsed = Hash::from_hex(&amp;hex).unwrap();
        assert_eq!(original, parsed);
    }

    #[test]
    fn test_json_canonicalization() {
        // Same data, different key order
        let v1 = json!({&#34;b&#34;: 1, &#34;a&#34;: 2});
        let v2 = json!({&#34;a&#34;: 2, &#34;b&#34;: 1});

        let h1 = ContentHasher::hash_json(&amp;v1);
        let h2 = ContentHasher::hash_json(&amp;v2);
        assert_eq!(h1, h2);
    }

    #[test]
    fn test_combine_hashes() {
        let h1 = ContentHasher::hash_string(&#34;a&#34;);
        let h2 = ContentHasher::hash_string(&#34;b&#34;);
        let combined = ContentHasher::combine(&amp;[h1.clone(), h2.clone()]);

        // Combining in different order should be different
        let combined2 = ContentHasher::combine(&amp;[h2, h1]);
        assert_ne!(combined, combined2);
    }

    #[test]
    fn test_hash_operation() {
        let params = json!({&#34;min_prevalence&#34;: 0.1, &#34;samples&#34;: [&#34;a&#34;, &#34;b&#34;]});
        let hash = ContentHasher::hash_operation(&#34;filter_prevalence&#34;, &amp;params);
        assert!(!hash.to_hex().is_empty());

        // Same params, same hash
        let params2 = json!({&#34;samples&#34;: [&#34;a&#34;, &#34;b&#34;], &#34;min_prevalence&#34;: 0.1});
        let hash2 = ContentHasher::hash_operation(&#34;filter_prevalence&#34;, &amp;params2);
        assert_eq!(hash, hash2);

        // Different operation, different hash
        let hash3 = ContentHasher::hash_operation(&#34;filter_abundance&#34;, &amp;params);
        assert_ne!(hash, hash3);
    }
}
