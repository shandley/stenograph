        let root = Version::root(session.id.clone(), &#34;counts |&gt; filter(0.1)&#34;);
        let child = Version::child(&amp;root, &#34;counts |&gt; filter(0.1) |&gt; normalize()&#34;);

        assert!(root.parent_id.is_none());
        assert_eq!(child.parent_id, Some(root.id.clone()));
        assert_eq!(child.session_id, session.id);
