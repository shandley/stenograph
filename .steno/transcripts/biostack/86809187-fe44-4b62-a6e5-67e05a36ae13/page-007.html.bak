<div class="truncatable"><div class="truncatable-content"><pre class="file-content">//! Core provenance graph types.
//!
//! These types represent the nodes, edges, and metadata in the provenance DAG.

use chrono::{DateTime, Utc};
use serde::{Deserialize, Serialize};
use std::collections::HashMap;

/// Unique identifier for a session.
#[derive(Debug, Clone, PartialEq, Eq, Hash, Serialize, Deserialize)]
pub struct SessionId(pub String);

impl SessionId {
    /// Create a new session ID.
    pub fn new(id: impl Into&lt;String&gt;) -&gt; Self {
        Self(id.into())
    }

    /// Generate a new unique session ID.
    pub fn generate() -&gt; Self {
        Self(uuid::Uuid::new_v4().to_string())
    }
}

impl std::fmt::Display for SessionId {
    fn fmt(&amp;self, f: &amp;mut std::fmt::Formatter&lt;&#39;_&gt;) -&gt; std::fmt::Result {
        write!(f, &#34;{}&#34;, self.0)
    }
}

/// Unique identifier for a version (expression snapshot).
#[derive(Debug, Clone, PartialEq, Eq, Hash, Serialize, Deserialize)]
pub struct VersionId(pub String);

impl VersionId {
    /// Create a new version ID.
    pub fn new(id: impl Into&lt;String&gt;) -&gt; Self {
        Self(id.into())
    }

    /// Generate a new unique version ID.
    pub fn generate() -&gt; Self {
        Self(uuid::Uuid::new_v4().to_string())
    }
}

impl std::fmt::Display for VersionId {
    fn fmt(&amp;self, f: &amp;mut std::fmt::Formatter&lt;&#39;_&gt;) -&gt; std::fmt::Result {
        write!(f, &#34;{}&#34;, self.0)
    }
}

/// Unique identifier for a node (primitive execution).
#[derive(Debug, Clone, PartialEq, Eq, Hash, Serialize, Deserialize)]
pub struct NodeId(pub String);

impl NodeId {
    /// Create a new node ID.
    pub fn new(id: impl Into&lt;String&gt;) -&gt; Self {
        Self(id.into())
    }

    /// Generate a new unique node ID.
    pub fn generate() -&gt; Self {
        Self(uuid::Uuid::new_v4().to_string())
    }
}

impl std::fmt::Display for NodeId {
    fn fmt(&amp;self, f: &amp;mut std::fmt::Formatter&lt;&#39;_&gt;) -&gt; std::fmt::Result {
        write!(f, &#34;{}&#34;, self.0)
    }
}

/// A research session with an intent.
///
/// Sessions group related analyses together and provide context
/// for the work being done.
#[derive(Debug, Clone, Serialize, Deserialize)]
pub struct Session {
    /// Unique session identifier.
    pub id: SessionId,

    /// Human-readable project name.
    pub project: String,

    /// Natural language description of the analysis intent.
    pub intent: String,

    /// When the session was created.
    pub created_at: DateTime&lt;Utc&gt;,

    /// When the session was last modified.
    pub updated_at: DateTime&lt;Utc&gt;,

    /// Optional metadata (user, environment, etc.).
    #[serde(default)]
    pub metadata: HashMap&lt;String, serde_json::Value&gt;,
}

impl Session {
    /// Create a new session.
    pub fn new(project: impl Into&lt;String&gt;, intent: impl Into&lt;String&gt;) -&gt; Self {
        let now = Utc::now();
        Self {
            id: SessionId::generate(),
            project: project.into(),
            intent: intent.into(),
            created_at: now,
            updated_at: now,
            metadata: HashMap::new(),
        }
    }

    /// Add metadata to the session.
    pub fn with_metadata(mut self, key: impl Into&lt;String&gt;, value: serde_json::Value) -&gt; Self {
        self.metadata.insert(key.into(), value);
        self
    }
}

/// A version represents a snapshot of an expression in the DAG.
///
/// Versions form a tree structure where each version has a parent
/// (except the root). This enables branching and exploration.
#[derive(Debug, Clone, Serialize, Deserialize)]
pub struct Version {
    /// Unique version identifier.
    pub id: VersionId,

    /// Session this version belongs to.
    pub session_id: SessionId,

    /// The DSL expression at this version.
    pub expression: String,

    /// Parent version (None for root).
    pub parent_id: Option&lt;VersionId&gt;,

    /// When this version was created.
    pub created_at: DateTime&lt;Utc&gt;,

    /// Hash of the expression for quick comparison.
    pub expression_hash: String,

    /// Optional message describing the change.
    pub message: Option&lt;String&gt;,
}

impl Version {
    /// Create a new root version.
    pub fn root(session_id: SessionId, expression: impl Into&lt;String&gt;) -&gt; Self {
        let expr = expression.into();
        let hash = crate::provenance::ContentHasher::hash_string(&amp;expr);
        Self {
            id: VersionId::generate(),
            session_id,
            expression: expr,
            parent_id: None,
            created_at: Utc::now(),
            expression_hash: hash.to_string(),
            message: None,
        }
    }

    /// Create a child version.
    pub fn child(parent: &amp;Version, expression: impl Into&lt;String&gt;) -&gt; Self {
        let expr = expression.into();
        let hash = crate::provenance::ContentHasher::hash_string(&amp;expr);
        Self {
            id: VersionId::generate(),
            session_id: parent.session_id.clone(),
            expression: expr,
            parent_id: Some(parent.id.clone()),
            created_at: Utc::now(),
            expression_hash: hash.to_string(),
            message: None,
        }
    }

    /// Add a message to the version.
    pub fn with_message(mut self, message: impl Into&lt;String&gt;) -&gt; Self {
        self.message = Some(message.into());
        self
    }
}

/// A node represents a single primitive execution.
///
/// Nodes are the core unit of the provenance graph. Each node
/// records what operation was performed, with what parameters,
/// and hashes of inputs/outputs for caching.
#[derive(Debug, Clone, Serialize, Deserialize)]
pub struct Node {
    /// Unique node identifier.
    pub id: NodeId,

    /// Session this node belongs to.
    pub session_id: SessionId,

    /// Version that created this node.
    pub version_id: Option&lt;VersionId&gt;,

    /// The primitive operation performed.
    pub operation: String,

    /// Parameters passed to the primitive (JSON).
    pub params: serde_json::Value,

    /// Hash of all inputs (for cache lookup).
    pub input_hash: String,

    /// Hash of all outputs (for verification).
    pub output_hash: Option&lt;String&gt;,

    /// When this node was created.
    pub created_at: DateTime&lt;Utc&gt;,

    /// Execution duration in milliseconds.
    pub duration_ms: Option&lt;u64&gt;,

    /// Status of the execution.
    pub status: NodeStatus,

    /// Error message if failed.
    pub error: Option&lt;String&gt;,
}

/// Status of a node execution.
#[derive(Debug, Clone, Copy, PartialEq, Eq, Serialize, Deserialize)]
#[serde(rename_all = &#34;lowercase&#34;)]
pub enum NodeStatus {
    /// Node is pending execution.
    Pending,
    /// Node is currently executing.
    Running,
    /// Node completed successfully.
    Success,
    /// Node failed with an error.
    Failed,
    /// Node was skipped (cache hit).
    Cached,
}

impl Default for NodeStatus {
    fn default() -&gt; Self {
        Self::Pending
    }
}

impl Node {
    /// Create a new node for an operation.
    pub fn new(session_id: SessionId, operation: impl Into&lt;String&gt;) -&gt; Self {
        Self {
            id: NodeId::generate(),
            session_id,
            version_id: None,
            operation: operation.into(),
            params: serde_json::Value::Null,
            input_hash: String::new(),
            output_hash: None,
            created_at: Utc::now(),
            duration_ms: None,
            status: NodeStatus::Pending,
            error: None,
        }
    }

    /// Set the version that created this node.
    pub fn with_version(mut self, version_id: VersionId) -&gt; Self {
        self.version_id = Some(version_id);
        self
    }

    /// Set the parameters.
    pub fn with_params(mut self, params: serde_json::Value) -&gt; Self {
        self.params = params;
        self
    }

    /// Set the input hash.
    pub fn with_input_hash(mut self, hash: impl Into&lt;String&gt;) -&gt; Self {
        self.input_hash = hash.into();
        self
    }

    /// Set the output hash.
    pub fn with_output_hash(mut self, hash: impl Into&lt;String&gt;) -&gt; Self {
        self.output_hash = Some(hash.into());
        self
    }

    /// Mark the node as running.
    pub fn start(&amp;mut self) {
        self.status = NodeStatus::Running;
    }

    /// Mark the node as successful.
    pub fn complete(&amp;mut self, duration_ms: u64, output_hash: impl Into&lt;String&gt;) {
        self.status = NodeStatus::Success;
        self.duration_ms = Some(duration_ms);
        self.output_hash = Some(output_hash.into());
    }

    /// Mark the node as failed.
    pub fn fail(&amp;mut self, error: impl Into&lt;String&gt;) {
        self.status = NodeStatus::Failed;
        self.error = Some(error.into());
    }

    /// Mark the node as cached (skipped).
    pub fn cache_hit(&amp;mut self, output_hash: impl Into&lt;String&gt;) {
        self.status = NodeStatus::Cached;
        self.output_hash = Some(output_hash.into());
        self.duration_ms = Some(0);
    }
}

/// An edge represents data flow between nodes.
///
/// Edges connect outputs of one node to inputs of another,
/// forming the DAG structure. Each edge is labeled with a slot
/// name and the hash of the data.
#[derive(Debug, Clone, Serialize, Deserialize)]
pub struct Edge {
    /// Source node (produces the data).
    pub from_node: NodeId,

    /// Target node (consumes the data).
    pub to_node: NodeId,

    /// Slot name (e.g., &#34;counts&#34;, &#34;metadata&#34;).
    pub slot: String,

    /// Hash of the data flowing through this edge.
    pub data_hash: String,
}

impl Edge {
    /// Create a new edge.
    pub fn new(
        from_node: NodeId,
        to_node: NodeId,
        slot: impl Into&lt;String&gt;,
        data_hash: impl Into&lt;String&gt;,
    ) -&gt; Self {
        Self {
            from_node,
            to_node,
            slot: slot.into(),
            data_hash: data_hash.into(),
        }
    }
}

/// Type of artifact produced by a node.
#[derive(Debug, Clone, Copy, PartialEq, Eq, Serialize, Deserialize)]
#[serde(rename_all = &#34;snake_case&#34;)]
pub enum ArtifactType {
    /// Count matrix data.
    CountMatrix,
    /// Sample metadata.
    SampleMetadata,
    /// Test results (p-values, etc.).
    TestResults,
    /// Ordination results (PCA, PCoA, etc.).
    Ordination,
    /// Diversity metrics.
    Diversity,
    /// Plot specification.
    Plot,
    /// Generic JSON data.
    Json,
    /// Binary data.
    Binary,
}

/// An artifact is a serialized output from a node.
///
/// Artifacts store the actual data produced by primitives,
/// enabling result caching and retrieval.
#[derive(Debug, Clone, Serialize, Deserialize)]
pub struct Artifact {
    /// Unique artifact identifier.
    pub id: String,

    /// Node that produced this artifact.
    pub node_id: NodeId,

    /// Type of artifact.
    pub artifact_type: ArtifactType,

    /// Slot name this artifact fills.
    pub slot: String,

    /// Content hash of the artifact.
    pub content_hash: String,

    /// File path (relative to .biostack/).
    pub file_path: Option&lt;String&gt;,

    /// Inline value (for small artifacts).
    pub inline_value: Option&lt;serde_json::Value&gt;,

    /// When this artifact was created.
    pub created_at: DateTime&lt;Utc&gt;,

    /// Size in bytes.
    pub size_bytes: Option&lt;u64&gt;,
}

impl Artifact {
    /// Create a new artifact with a file path.
    pub fn from_file(
        node_id: NodeId,
        artifact_type: ArtifactType,
        slot: impl Into&lt;String&gt;,
        content_hash: impl Into&lt;String&gt;,
        file_path: impl Into&lt;String&gt;,
    ) -&gt; Self {
        Self {
            id: uuid::Uuid::new_v4().to_string(),
            node_id,
            artifact_type,
            slot: slot.into(),
            content_hash: content_hash.into(),
            file_path: Some(file_path.into()),
            inline_value: None,
            created_at: Utc::now(),
            size_bytes: None,
        }
    }

    /// Create a new artifact with an inline value.
    pub fn from_value(
        node_id: NodeId,
        artifact_type: ArtifactType,
        slot: impl Into&lt;String&gt;,
        content_hash: impl Into&lt;String&gt;,
        value: serde_json::Value,
    ) -&gt; Self {
        let size = serde_json::to_string(&amp;value)
            .map(|s| s.len() as u64)
            .ok();
        Self {
            id: uuid::Uuid::new_v4().to_string(),
            node_id,
            artifact_type,
            slot: slot.into(),
            content_hash: content_hash.into(),
            file_path: None,
            inline_value: Some(value),
            created_at: Utc::now(),
            size_bytes: size,
        }
    }

    /// Set the size in bytes.
    pub fn with_size(mut self, size_bytes: u64) -&gt; Self {
        self.size_bytes = Some(size_bytes);
        self
    }
}

