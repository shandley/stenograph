   614‚Üí            // Load and display data
   615‚Üí            function loadData() {
   616‚Üí                console.log(&#x27;Loading tool comparison data...&#x27;);
   617‚Üí                
   618‚Üí                try {
   619‚Üí                    const baseUrl = window.location.href.split(&#x27;/&#x27;).slice(0, -1).join(&#x27;/&#x27;) + &#x27;/&#x27;;
   620‚Üí                    const dataUrl = new URL(&#x27;data.json&#x27;, baseUrl).href;
   621‚Üí                    const impactDataUrl = new URL(&#x27;impact_data.json&#x27;, baseUrl).href;
   622‚Üí                    
   623‚Üí                    console.log(&#x27;Fetching tool data from:&#x27;, dataUrl);
   624‚Üí                    console.log(&#x27;Fetching impact data from:&#x27;, impactDataUrl);
   625‚Üí                    
   626‚Üí                    // Use Promise.all to fetch both data files in parallel
   627‚Üí                    Promise.all([
   628‚Üí                        fetch(`${dataUrl}?t=${Date.now()}`),
   629‚Üí                        fetch(`${impactDataUrl}?t=${Date.now()}`)
   630‚Üí                    ])
   631‚Üí                    .then(responses =&gt; {
   632‚Üí                        // Check if both responses are OK
   633‚Üí                        if (!responses[0].ok) {
   634‚Üí                            throw new Error(`Failed to load data.json (Status: ${responses[0].status})`);
   635‚Üí                        }
   636‚Üí                        if (!responses[1].ok) {
   637‚Üí                            throw new Error(`Failed to load impact_data.json (Status: ${responses[1].status})`);
   638‚Üí                        }
   639‚Üí                        
   640‚Üí                        // Parse both responses as JSON
   641‚Üí                        return Promise.all([
   642‚Üí                            responses[0].json(),
   643‚Üí                            responses[1].json()
   644‚Üí                        ]);
   645‚Üí                    })
   646‚Üí                    .then(([toolData, impactData]) =&gt; {
   647‚Üí                        console.log(&#x27;Tool data and impact data loaded successfully&#x27;);
   648‚Üí                        
   649‚Üí                        // Process and merge data
   650‚Üí                        const tools = toolData.nodes.filter(node =&gt; node.type === &#x27;tool&#x27;);
   651‚Üí                        const impactTools = impactData.tools;
   652‚Üí                        
   653‚Üí                        console.log(`Found ${tools.length} tools in data.json`);
   654‚Üí                        console.log(`Found ${impactTools.length} tools in impact_data.json`);
   655‚Üí                        
   656‚Üí                        // Create impact data lookup map for faster access
   657‚Üí                        const impactMap = {};
   658‚Üí                        impactTools.forEach(tool =&gt; {
   659‚Üí                            impactMap[tool.name] = tool;
   660‚Üí                        });
   661‚Üí                        
   662‚Üí                        // Merge data from both sources
   663‚Üí                        const mergedTools = tools.map(tool =&gt; {
   664‚Üí                            // Format topic/features from description or topics
   665‚Üí                            let keyFeatures = [];
   666‚Üí                            
   667‚Üí                            // Extract features from description (format: &quot;[feature1] [feature2]&quot;)
   668‚Üí                            const descriptionMatch = tool.description &amp;&amp; tool.description.match(/\[(.*?)\]/g);
   669‚Üí                            if (descriptionMatch) {
   670‚Üí                                keyFeatures = descriptionMatch.map(m =&gt; m.replace(/[\[\]]/g, &#x27;&#x27;).trim())
   671‚Üí                                    .filter(f =&gt; f &amp;&amp; f.length &gt; 1 &amp;&amp; !f.match(/^v\d/)); // Filter out version numbers
   672‚Üí                            }
   673‚Üí                            
   674‚Üí                            // Add topics if available and not already included
   675‚Üí                            if (tool.topics &amp;&amp; Array.isArray(tool.topics)) {
   676‚Üí                                tool.topics.forEach(topic =&gt; {
   677‚Üí                                    if (topic &amp;&amp; !keyFeatures.includes(topic)) {
   678‚Üí                                        keyFeatures.push(topic);
   679‚Üí                                    }
   680‚Üí                                });
   681‚Üí                            }
   682‚Üí                            
   683‚Üí                            // If we still don&#x27;t have features, extract words from description
   684‚Üí                            if (keyFeatures.length === 0 &amp;&amp; tool.description) {
   685‚Üí                                const words = tool.description.split(/[,\.\s]+/);
   686‚Üí                                const uniqueWords = [...new Set(words.filter(w =&gt; w.length &gt; 5))];
   687‚Üí                                keyFeatures = uniqueWords.slice(0, 3);
   688‚Üí                            }
   689‚Üí                            
   690‚Üí                            // Limit to 5 features max
   691‚Üí                            keyFeatures = keyFeatures.slice(0, 5);
   692‚Üí                            
   693‚Üí                            // Calculate total citations if available in impact data
   694‚Üí                            let totalCitations = 0;
   695‚Üí                            let influentialCitations = 0;
   696‚Üí                            const impactTool = impactMap[tool.name];
   697‚Üí                            
   698‚Üí                            if (impactTool) {
   699‚Üí                                if (impactTool.citations_by_year) {
   700‚Üí                                    totalCitations = Object.values(impactTool.citations_by_year)
   701‚Üí                                        .reduce((sum, count) =&gt; sum + (Number(count) || 0), 0);
   702‚Üí                                }
   703‚Üí                                influentialCitations = impactTool.influential_citations || 0;
   704‚Üí                            }
   705‚Üí                            
   706‚Üí                            // Determine maintenance status based on lastUpdated date
   707‚Üí                            let maintenanceStatus = &#x27;inactive&#x27;;
   708‚Üí                            if (tool.lastUpdated) {
   709‚Üí                                const updateDate = new Date(tool.lastUpdated);
   710‚Üí                                const now = new Date();
   711‚Üí                                const monthsDiff = (now.getFullYear() - updateDate.getFullYear()) * 12 + 
   712‚Üí                                    now.getMonth() - updateDate.getMonth();
   713‚Üí                                
   714‚Üí                                if (monthsDiff &lt; 6) {
   715‚Üí                                    maintenanceStatus = &#x27;active&#x27;;
   716‚Üí                                } else if (monthsDiff &lt; 12) {
   717‚Üí                                    maintenanceStatus = &#x27;maintained&#x27;;
   718‚Üí                                }
   719‚Üí                            }
   720‚Üí                            
   721‚Üí                            // Extract package manager from description or use default
   722‚Üí                            let packageManager = &#x27;source&#x27;;
   723‚Üí                            if (tool.description) {
   724‚Üí                                if (tool.description.includes(&#x27;[conda]&#x27;)) packageManager = &#x27;conda&#x27;;
   725‚Üí                                else if (tool.description.includes(&#x27;[pip]&#x27;)) packageManager = &#x27;pip&#x27;;
   726‚Üí                                else if (tool.description.includes(&#x27;[bioconda]&#x27;)) packageManager = &#x27;conda&#x27;;
   727‚Üí                                
   728‚Üí                                // Handle multiple package managers
   729‚Üí                                if (tool.description.includes(&#x27;[conda, pip]&#x27;) || 
   730‚Üí                                    tool.description.includes(&#x27;[pip, conda]&#x27;)) {
   731‚Üí                                    packageManager = &#x27;conda, pip&#x27;;
   732‚Üí                                }
   733‚Üí                            }
   734‚Üí                            
   735‚Üí                            // Extract main language
   736‚Üí                            let mainLanguage = &#x27;Unknown&#x27;;
   737‚Üí                            if (tool.language) {
   738‚Üí                                mainLanguage = tool.language;
   739‚Üí                            } else if (tool.languages &amp;&amp; Object.keys(tool.languages).length &gt; 0) {
   740‚Üí                                // Sort languages by usage and use the top one
   741‚Üí                                const sortedLangs = Object.entries(tool.languages)
   742‚Üí                                    .sort((a, b) =&gt; b[1] - a[1]);
   743‚Üí                                mainLanguage = sortedLangs[0][0];
   744‚Üí                            }
   745‚Üí                            
   746‚Üí                            return {
   747‚Üí                                name: tool.name,
   748‚Üí                                category: tool.category || &#x27;Uncategorized&#x27;,
   749‚Üí                                stars: tool.stars || 0,
   750‚Üí                                citations: totalCitations,
   751‚Üí                                influentialCitations: influentialCitations,
   752‚Üí                                lastUpdated: tool.lastUpdated || null,
   753‚Üí                                maintenanceStatus: maintenanceStatus,
   754‚Üí                                language: mainLanguage,
   755‚Üí                                packageManager: tool.package_manager || packageManager,
   756‚Üí                                keyFeatures: keyFeatures,
   757‚Üí                                url: tool.url || &#x27;#&#x27;,
   758‚Üí                                description: tool.description || &#x27;No description available.&#x27;
   759‚Üí                            };
   760‚Üí                        });
   761‚Üí                        
   762‚Üí                        // Sort by stars (descending) by default
   763‚Üí                        mergedTools.sort((a, b) =&gt; b.stars - a.stars);
   764‚Üí                        
   765‚Üí                        processData(mergedTools);
   766‚Üí                    })
   767‚Üí                    .catch(error =&gt; {
   768‚Üí                        console.error(&#x27;Error loading data:&#x27;, error);
   769‚Üí                        // Display error in table
   770‚Üí                        document.getElementById(&#x27;comparisonTableBody&#x27;).innerHTML = `
   771‚Üí                            &lt;tr&gt;
   772‚Üí                                &lt;td colspan=&quot;10&quot; class=&quot;text-center&quot;&gt;
   773‚Üí                                    &lt;div class=&quot;alert alert-danger my-3&quot;&gt;
   774‚Üí                                        &lt;i class=&quot;bi bi-exclamation-triangle-fill me-2&quot;&gt;&lt;/i&gt;
   775‚Üí                                        Error loading data: ${error.message}
   776‚Üí                                    &lt;/div&gt;
   777‚Üí                                    &lt;p&gt;Please refresh the page to try again.&lt;/p&gt;
   778‚Üí                                &lt;/td&gt;
   779‚Üí                            &lt;/tr&gt;
   780‚Üí                        `;
   781‚Üí                    });
   782‚Üí                } catch (error) {
   783‚Üí                    console.error(&#x27;Error in data fetch setup:&#x27;, error);
   784‚Üí                    document.getElementById(&#x27;comparisonTableBody&#x27;).innerHTML = `
   785‚Üí                        &lt;tr&gt;
   786‚Üí                            &lt;td colspan=&quot;10&quot; class=&quot;text-center&quot;&gt;
   787‚Üí                                &lt;div class=&quot;alert alert-danger my-3&quot;&gt;
   788‚Üí                                    &lt;i class=&quot;bi bi-exclamation-triangle-fill me-2&quot;&gt;&lt;/i&gt;
   789‚Üí                                    Error setting up data fetch: ${error.message}
   790‚Üí                                &lt;/div&gt;
   791‚Üí                                &lt;p&gt;Please refresh the page to try again.&lt;/p&gt;
   792‚Üí                            &lt;/td&gt;
   793‚Üí                        &lt;/tr&gt;
   794‚Üí                    `;
   795‚Üí                }
   796‚Üí            }
