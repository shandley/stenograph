   574‚Üí    &lt;!-- Main Script --&gt;
   575‚Üí    &lt;script&gt;
   576‚Üí        document.addEventListener(&#x27;DOMContentLoaded&#x27;, function() {
   577‚Üí            console.log(&#x27;Comparison tool initializing...&#x27;);
   578‚Üí            
   579‚Üí            // Theme handling
   580‚Üí            const themeToggle = document.getElementById(&#x27;theme-toggle&#x27;);
   581‚Üí            const themeIcon = themeToggle.querySelector(&#x27;i&#x27;);
   582‚Üí            
   583‚Üí            // Check for saved theme
   584‚Üí            const savedTheme = localStorage.getItem(&#x27;theme&#x27;);
   585‚Üí            if (savedTheme) {
   586‚Üí                document.body.setAttribute(&#x27;data-theme&#x27;, savedTheme);
   587‚Üí                updateThemeIcon(savedTheme);
   588‚Üí            }
   589‚Üí            
   590‚Üí            // Toggle theme
   591‚Üí            themeToggle.addEventListener(&#x27;click&#x27;, function() {
   592‚Üí                const currentTheme = document.body.getAttribute(&#x27;data-theme&#x27;) || &#x27;light&#x27;;
   593‚Üí                const newTheme = currentTheme === &#x27;dark&#x27; ? &#x27;light&#x27; : &#x27;dark&#x27;;
   594‚Üí                
   595‚Üí                document.body.setAttribute(&#x27;data-theme&#x27;, newTheme);
   596‚Üí                localStorage.setItem(&#x27;theme&#x27;, newTheme);
   597‚Üí                updateThemeIcon(newTheme);
   598‚Üí            });
   599‚Üí            
   600‚Üí            function updateThemeIcon(theme) {
   601‚Üí                if (theme === &#x27;dark&#x27;) {
